                                                                            <tr>
                                                                              <td
                                                                                style="padding:10px 30px;border-radius:2px;font-family:'Open Sans',Arial,sans-serif;text-align:center;font-size:14px;color:#ffffff;background-color:#242d34"
                                                                                bgcolor="#242D34"><a
                                                                                  style="color:red;font-size:14px;text-decoration:none;font-weight:600"
                                                                                  href="{{loginUrl}}" target="_blank">
                                                                                  <span
                                                                                    style="color:#ffffff;text-decoration:none;font-size:14px;font-weight:600">Log-in</span>
                                                                                </a>
                                                                              </td>
                                                                            </tr>
                                                                          </tbody>
                                                                        </table>
                                                                      </td>
                                                                    </tr>

                                                                    <tr>
                                                                      <td style="font-size:1px" height="5">&nbsp;</td>
